<%= simple_form_for @holder do |f| %>
  <%= f.input :full_name %>
  <%= f.input :date_of_birth, as: :date, start_year: Date.today.year - 90,
                                   end_year: Date.today.year - 16  %>
  <%= f.input :date_of_car_license, as: :date, start_year: Date.today.year - 90,
                                   end_year: Date.today.year %>
  <%= f.button :submit %>
<% end %>

