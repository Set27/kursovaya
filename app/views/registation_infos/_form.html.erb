<%= simple_form_for @registation do |f| %>
  <%= f.input :state_number %>
  <%= f.association :car_holder_info, label_method: :full_name, value_method: :id %>        
  <%= f.association :car_info, label_method: :VIN, value_method: :id %>
  <%= f.input :date_of_registation, as: :date, start_year: Date.today.year - 90,
                                   end_year: Date.today.year %>
  <%= f.button :submit %>
<% end %>

